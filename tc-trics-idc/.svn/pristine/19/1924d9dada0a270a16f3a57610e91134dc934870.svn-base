<div id="operatePCDiv" style="display: none;">
    <shiro:hasPermission name="device:print">
        <button id="bind" type="button" class="btn bg-yellow table-btn" onclick="printQrPc('+row.id+')">打印</button>
    </shiro:hasPermission>
</div>
<div id="operatePhoneDiv" style="display: none;">
    <shiro:hasPermission name="device:print">
        <button id="bind1"  type="button" class="btn bg-yellow table-btn" onclick='printQrPhone(" +row.id + ")'>打印</button>
    </shiro:hasPermission>


</div>
<style>
  .qrcodeCanvas{display:none;page-break-after: always;}
</style>
<style media="print">
  .qrcodeCanvas{display:block;}
</style>
<body style="border: none;background: none" class="gdt">
<div class="row row-top">
      <div class="col-lg-12">
          <section class="panel">
              <header class="panel-heading">
                  <span class="tab-o">设备信息管理</span><b style="color: #00C6FF"> > </b><span class="tab-er">设备贴标</span>
              </header>
          </section>
      </div>
</div>

<div class="row-fulid">
  <section class="panel">
      <header class="panel-heading tab-bg-dark-navy-blue">
          <ul class="nav nav-tabs">
              <li class="active">
                  <a data-toggle="tab" href="#home">pc端贴标</a>
              </li>
              <li class="">
                  <a data-toggle="tab" href="#about">手持端贴标</a>
              </li>
          </ul>
      </header>
      <div class="panel-body">
          <div class="tab-content">
              <div id="home" class="tab-pane active">

                  <div class="row" style="padding: 15px 30px">
                      <button type="button" class="btn bg-yellow" data-toggle="modal" data-target="#openFilter">筛选</button>
                      <button type="button" shiro:hasPermission="device:batchPrint" class="btn bg-yellow" name="pc_batch_print">批量打印</button>
                      <button type="button" class="btn bg-yellow" shiro:hasPermission="device:batchConfirm" name="pc_batch_confim">确认贴标完成</button>
                      <button type="button" shiro:hasPermission="device:bindPcExport"  class="btn bg-yellow" name="pc_ex">导出</button>
                      <form id="pc_exForm" th:action="@{/device/bindPcExport}" method="post"></form>
                  </div>
                  <div class="row-fulid">
                      <section class="panel1">
                          <table id="pcTable"></table>
                      </section>
                  </div>
              </div>
              <div id="about" class="tab-pane">
                  <div class="row" style="padding: 15px 30px">
                      <button type="button" class="btn bg-yellow" data-toggle="modal" data-target="#openFilter">筛选</button>
                      <button type="button" shiro:hasPermission="device:batchPrint" class="btn bg-yellow" name="phone_batch_print">批量打印</button>
                      <button type="button" shiro:hasPermission="device:bindPhoneExport" class="btn bg-yellow" name="phone_ex">导出</button>
                      <form id="phone_exForm" th:action="@{/device/bindPhoneExport}" method="post"></form>
                  </div>
                  <div class="row">
                      <div class="col-sm-12">
                          <section class="">
                              <table id="phoneTable"></table>
                          </section>
                      </div>
                  </div>
              </div>
          </div>
      </div>
  </section>
</div>
<div id="print"></div>

<section>
  <div class="modal fade" id="openFilter" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
      <div class="modal-dialog" style="width: 90%">
          <div class="modal-content">
              <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                      &times;
                  </button>
                  <h4 class="modal-title">
                      筛选详情
                  </h4>
              </div>
              <div class="modal-body">
                  <form id="queryForm">
                  <div class="row" id="queryRow">
                      <div class=" form-group col-sm-2">
                          <label class="">设备名称</label>
                          <input type="text"  name="name" class="form-control" placeholder="设备名称">
                      </div>
                      <div class=" form-group col-sm-2">
                          <label >所属机构</label>
                          <div class="">
                              <select class="selectpicker show-tick form-control" data-live-search="true" name="orgId">
                              </select>
                          </div>
                      </div>
                      <div class=" form-group col-sm-2">
                          <label >机房</label>
                          <div class="">
                              <select class="selectpicker show-tick form-control" data-live-search="true" name="roomId">
                                  <option value="">请先选择机构</option>
                              </select>
                          </div>
                      </div>
                      <div class="form-group col-sm-2">
                          <label >机架</label>
                          <select class="selectpicker show-tick form-control" data-live-search="true" name="rackId">
                              <option value="">请先选择机房</option>
                          </select>
                      </div>
                      <div class=" form-group col-sm-2">
                          <label >设备类型</label>
                          <div class="">
                              <select class="selectpicker show-tick form-control" data-live-search="true" name="resTypeCode">
                              </select>
                          </div>
                      </div>
                      <div class="form-group col-sm-2">
                          <label >状态</label>
                          <div class="">
                              <select class="selectpicker show-tick form-control" data-live-search="true" name="status">
                              </select>
                          </div>
                      </div>
                      <div class="form-group col-sm-2">
                          <label >标签编号</label>
                          <input  class="form-control"  placeholder="标签编号" name="bacode">
                      </div>
                      <div class="form-group col-sm-2">
                          <label >资产编号</label>
                          <input  class="form-control"  placeholder="资产编号" name="assetId">
                      </div>
                      <div class="form-group col-sm-2">
                          <label >TID</label>
                          <input  class="form-control"  placeholder="TID" name="tid">
                      </div>
                      <div class="form-group col-sm-2">
                          <label >所有人</label>
                          <input  class="form-control"  placeholder="所有人" name="owner">
                      </div>
                      <div class="form-group col-sm-2">
                          <label >操作员</label>
                          <input  class="form-control"  placeholder="操作员" name="operator">
                      </div>
                  </div>
                  </form>
              </div>

              <div class="data">

              </div>
              <div class="modal-footer">
                  <button type="button" class="btn btn-default" data-dismiss="modal" name="close">关闭
                  </button>
                  <button type="button" class="btn btn-primary" name="search_btn">
                      确认筛选
                  </button>
              </div>
          </div><!-- /.modal-content -->
      </div><!-- /.modal -->
  </div>
    <div id="batch_print"></div>
    <!--<div class="row">
        <div class="col-lg-3 visible-print-block">1</div>
        <div class="col-lg-3 visible-print-inline">2</div>
        <div class="col-lg-3 visible-print-inline-block">3</div>
        <div class="col-lg-3 hidden-print">4</div>
    </div>-->
</section>
<div  id="wrap" style="width: 100px;height: 100px;border: #3D3D3D 1px solid;visibility: hidden">
</div>

