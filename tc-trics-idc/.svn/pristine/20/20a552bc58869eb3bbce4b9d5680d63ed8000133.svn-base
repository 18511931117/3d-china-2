                <div class="row" style="margin-top: 15px">
                    <div class="col-sm-6">
                        <form class="form-horizontal tasi-form" method="get">
                            <div class="form-group col-sm-12">
                                <label class="control-label col-sm-4">任务名称</label>
                                <div class="col-sm-6">
                                    <input type="text" id="taskName" class="form-control" placeholder="任务名称">
                                </div>
                            </div>
                            <div class="form-group col-sm-12">
                                <label class="control-label col-sm-4">备注</label>
                                <div class="col-sm-6">
                                    <textarea class="form-control" id="remark" rows="3" style="height: 50px"></textarea>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-sm-6">
                        <form class="form-horizontal tasi-form" method="get">
                            <div class="form-group">
                                <label class="control-label col-sm-4">部门</label>
                                <div class="col-sm-8">
                                    <select class="selectpicker show-tick form-control" data-live-search="true" name="loadOrg"  id="loadOrg">

                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-4">负责人</label>
                                <div class="col-sm-8">
                                    <select class="selectpicker show-tick form-control" data-live-search="true" name="loadUser"  id="userId">

                                    </select>
                                </div>
                            </div>
                            <div class="form-group" id="startRoomNone">
                                <label class="control-label col-sm-4">存放位置</label>
                                <div class="col-sm-8">
