<body style="border: none;background: none" class="gdt">

<div id="operateDiv" style="display: none;">
    <shiro:hasPermission name="resType:updateResType">
        <!-- <button id="unbind" type="button" class="btn bg-yellow table-btn" onclick="updateModel('+value+')">编辑</button>-->
        <p id="editFlag">1</p>
    </shiro:hasPermission>
    <shiro:hasPermission name="resType:delResType">
        <!-- <button id="unbind1" type="button" class="btn bg-bold table-btn del" onclick="del('+value+')">删除</button>-->
        <p id="delFlag">1</p>
    </shiro:hasPermission>
</div>

<!-- page start-->
<div class="row row-top">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                <span class="tab-o">系统管理</span><b style="color: #00C6FF"> > </b><span class="tab-er">设备分类管理</span>
            </header>
        </section>
    </div>
</div>


<div class="table-warp">
    <div class="row row-pading">
        <div class="col-sm-10">
            <button type="button" class="btn bg-yellow" data-toggle="modal" data-target="#myModal">添加设备</button>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <section class="panel1">
                <table id="table"></table>
            </section>
        </div>
    </div>
</div>



<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="width: 60%">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="myModalLabel">
                    添加设备类型
                </h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal tasi-form" id="add">
                    <div class="form-group">
                        <label class="control-label col-sm-2">分类名称</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control verification" name="resTypeName" placeholder="请输入分类名称">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">编号</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control verification" name="resTypeCode" placeholder="请输入编号">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">上级分类</label>
                        <div class="col-sm-10">
                            <input type="hidden" name="pid" placeholder="请选择上级分类" class="verification">
                            <div name="panId"></div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" name="close">关闭
                </button>
                <button type="button" class="btn btn-primary" name="addButton">
                    提交更改
                </button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>




<button type="button" class="btn bg-yellow" id="updateModeButton" style="display: none" data-toggle="modal" data-target="#content3814"></button>
<div class="modal fade" id="content3814" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="width: 90%">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title">
                    编辑设备
                </h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal tasi-form" id="updateForm">
                    <input type="hidden" name="id">
                    <div class="form-group">
                        <label class="control-label col-sm-2">级别</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control verification" disabled name="level"  placeholder="级别">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">分类名称</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control verification" name="resTypeName" placeholder="请输入分类名称" >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">编号</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control verification" name="resTypeCode" placeholder="编号" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">上级分类</label>
                        <div class="col-sm-10">
                            <input type="hidden" name="pid" class="verification" placeholder="请选择上级分类">
                            <div name="panId"></div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" name="close">关闭
                </button>
                <button type="button" class="btn btn-primary" name="commit_btn">
                    确认编辑
                </button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>

<script type="text/javascript" th:inline="javascript">
    ctxPath = /*[[@{/}]]*/ '';
</script>
