// 添加任务
function task_submit() {
    var check_data = []
    var i = 0
    $.each($("input:checkbox"),function () {
        if(this.checked){
            check_data[i] = $(this).val()
            i = i+1
            chref = "create.html?res_id=" + check_data;
            window.location.href = chref;
        }

    })
}
// 加入任务
function addtask_submit() {
    var check_data = []
    var i = 0
    $.each($("input:checkbox"),function () {
        if(this.checked){
            check_data[i] = $(this).val()
            i = i+1

        }
        chref = "add_task.html?res_id=" + check_data;
        window.location.href = chref;
    })
}

