            return false
        }
        for (var i = 0; i< idlist.length;i++){
            $("#exportForm").append("<input type='hidden' name='ids' value='"+idlist[i].id+"'>");
        }
        $("#exportForm").submit();
        $("#exportForm").html("");
    });
    
    $("button[name='down_button']").click(function () {
