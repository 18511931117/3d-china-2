/*
    加载机架下拉框
 */
function loadRackSelect(ele,roomId,val){
    if(roomId != ''){
        ele.html("<option value=''>请选择</option>");
        $.ajax({
            url:ctxPath +"rack/getPage",
            type:'get',
            data:{'roomId':roomId},
            async: false,
            success:function(data){
                if(data.code == '0'){
                    for (var i = 0;i<data.rows.length;i++){
                        var obj = data.rows[i];
                        ele.append("<option value='"+obj.id+"'>"+obj.name+"</option>");
                    }
                    if(val != null && val != undefined && val != ''){
                        ele.val(val);
                    }
                }
            }
    })
    }else{
        ele.html("<option value=''>请先选择机房</option>");
    }
}



/*
    加载设备下拉菜单
 */
function loadDevSelect(name,rackId,pid,val){
    var ele;
    if(pid != null && pid != undefined && pid != ''){
        ele = $("#"+pid).find(":input[name='"+name+"']");
    }else{
        ele = $(":input[name='"+name+"']");
    }
    if(rackId != ''){
        ele.html("<option value=''>请选择</option>");
        $.get(
            ctxPath +"device/getDevicePage",
            {'rackId':rackId},
            function(data){
                if(data.code == '0'){
                    for (var i = 0;i<data.data.length;i++){
                        var obj = data.data[i];
                        ele.append("<option value='"+obj.id+"'>"+obj.name+"</option>");
                    }

                    if(val != null && val != undefined && val != ''){
                        ele.val(val);
                    }
                }
            })
    }else{
        ele.html("<option value=''>请先选择机房</option>");
    }
}

