<body style="border: none;background: none" class="gdt">
<div id="operateDiv" style="display: none;">
    <shiro:hasPermission name="org:update">
        <button id="editOrg1" type="button" class="btn bg-yellow table-btn" >编辑</button>
    </shiro:hasPermission>
    <shiro:hasPermission name="org:delete">
        <button id="deleteOrg" type="button" class="btn bg-bold table-btn">删除</button>
    </shiro:hasPermission>
</div>



<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModal" aria-hidden="true">
    <div class="modal-dialog" style="width: 60%">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title"  id="myModalLabel">
                    添加机构
                </h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal tasi-form" id="addForm" method="get">
                    <div class="form-group" style="display:none;">
                        <label class="control-label col-lg-2">级别</label>
                        <div class="col-sm-10">
                            <input type="hidden" name="parentFullCode">
                            <input type="text" name="level" class="form-control" disabled   placeholder="级别">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">机构名称</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="name" placeholder="机构名称">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">编码</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="fullCode" placeholder="编码">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">所属区域</label>
                        <div class="col-sm-10">
                            <select class="selectpicker show-tick form-control" data-live-search="true"  id="regionId" name="regionSelect">

                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">上级机构</label>
                        <div class="col-sm-10">
                            <input type="hidden" id="parentId">
                            <div id="orgTree"></div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭
                </button>
                <button type="button" class="btn btn-primary"  id="addOrgs">
                    提交
                </button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>

<div class="modal fade" id="editOrg" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="width: 60%">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="">
                    编辑机构
                </h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal tasi-form" method="get">
<!--                    <div class="form-group">-->
<!--                        <label class="control-label col-lg-2">级别</label>-->
<!--                        <div class="col-sm-10">-->
<!--                            <input type="hidden" name="id">-->
<!--                            <input type="text" name="levels" class="form-control" disabled   placeholder="级别">-->
<!--                        </div>-->
<!--                    </div>-->
                    <div class="form-group">
                        <label class="control-label col-sm-2">机构名称</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="names" placeholder="机构名称">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">编码</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="fullCodes" placeholder="编码">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">所属区域</label>
                        <div class="col-sm-10">
                            <select class="selectpicker show-tick form-control" data-live-search="true" id="regionIds" name="regionSelect">

                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">上级机构</label>
                        <div class="col-sm-10">
                            <input type="hidden" id="orgId">
                            <input type="hidden" id="parentIds">
                            <div id="orgTrees"></div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭
                </button>
                <button type="button" class="btn btn-primary" shiro:hasPermission="org:update"  id="editOrgs">
                    提交
                </button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>
<!-- page start-->
<div class="row row-top">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                <span class="tab-o">系统管理</span><b style="color: #00C6FF"> > </b><span class="tab-er">机构管理</span>
            </header>
        </section>
    </div>
</div>
<div class="table-warp">
    <div class="row row-pading">
        <div class="col-sm-12">
            <button type="button" shiro:hasPermission="org:add" class="btn bg-yellow" data-toggle="modal" data-target="#myModal">新建机构</button>
        </div>
    </div>


    <div class="row">
        <div class="col-lg-12">
            <section class="panel1">
                <table id="treeTable" ></table>
            </section>
        </div>
    </div>
</div>

