function getQuery () {
    var data = $("#formTestFilter").serializeObject();
    var index = 0;
    $("#formTestFilter").find(".ext").each(function () {
        data['resExts[' + index +'].itemCode'] = $(this).attr("name");
        data['resExts[' + index +'].itemValue'] = $(this).val();
        index++;
    });
    return data;
}



