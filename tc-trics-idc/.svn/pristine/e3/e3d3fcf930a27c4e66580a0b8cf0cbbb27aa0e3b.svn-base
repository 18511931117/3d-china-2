  <select id="selectByCondition" parameterType="map" resultMap="BaseResultMap">
    SELECT
    ct.*,so.`name` orgName,
    su.name ownerName
    FROM
    repair_task ct
    LEFT JOIN sys_org so ON so.id = ct.org_id
    LEFT JOIN  sys_user su on su.id=ct.user_id
    <where>
      <if test="fullCode != null">
        and so.full_code like concat('',#{fullCode},'%')
      </if>
      <if test="data.isDel!=null">
        and ct.is_del=#{data.isDel}
      </if>
      <if test="data.state!=null">
        and ct.state=#{data.state}
      </if>
      <if test="data.taskName!=null and data.taskName!=''">
        and ct.task_name like concat('%',#{data.taskName},'%')
      </if>
      and ct.state !=2
      ORDER BY create_time DESC
    </where>
  </select>
    <select id="getRepairTaskPage" resultMap="BaseResultMap">
      SELECT
      ct.*,so.`name` orgName,
      su.name ownerName,
      rm.name roomName
      FROM
      repair_task ct
      LEFT JOIN sys_org so ON so.id = ct.org_id
      LEFT JOIN sys_user su ON su.id=ct.user_id
      LEFT JOIN room rm ON rm.id=ct.room_id
      <where>
        <if test="fullCode != null">
          and so.full_code like concat('',#{fullCode},'%')
        </if>
        <if test="data.isDel!=null">
          and ct.is_del=#{data.isDel}
        </if>
        <if test="data.state!=null">
          and ct.state=#{data.state}
        </if>
        <if test="data.queryName!=null">
          and (ct.task_name like concat('%',#{data.queryName},'%') or ct.id like concat('%',#{data.queryName},'%'))
        </if>
        and ct.is_del=0
        and ct.state !=2
        ORDER BY create_time DESC
      </where>
    </select>
  <select id="getByTask" resultMap="BaseResultMap">
      SELECT
      ct.*,so.`name` orgName,
      su.name ownerName,
      rm.name roomName
      FROM
      repair_task ct
      LEFT JOIN sys_org so ON so.id = ct.org_id
      LEFT JOIN  sys_user su on su.id=ct.user_id
      LEFT JOIN  room rm on ct.room_id=rm.id
      <where>
        <if test="orgId!=null">
          and  (so.parent_id=#{orgId} or so.id=#{orgId})
        </if>
        <if test="isDel!=null">
          and ct.is_del=#{isDel}
        </if>
        <if test="state!=null">
          and ct.state=#{state}
        </if>
        <if test="taskName!=null">
          and ct.taskName like concat('%',#{taskName},'%')
        </if>
        and ct.id = #{id}
      </where>
    ORDER BY create_time DESC
